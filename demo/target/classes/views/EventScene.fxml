<StackPane fx:id="stackpane" xmlns="http://javafx.com/javafx/23.0.1" stylesheets="@style/style.css" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.quickStart.Controllers.EventController">

  <AnchorPane fx:id="mainPane" prefHeight="592.0" prefWidth="989.0"  xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" >
            <TableView fx:id="table_events" layoutY="172.0" prefHeight="420.0" prefWidth="989.0">
                <columns>
                    <TableColumn fx:id="event_name" minWidth="193.6" prefWidth="75.0" text="EVENT NAME" />
                    <TableColumn fx:id="date" minWidth="193.6" prefWidth="96.0" text="DATE" />
                    <TableColumn fx:id="description" minWidth="193.6" prefWidth="54.0" text="DESCRIPTION" />
                    <TableColumn fx:id="status" minWidth="193.6" prefWidth="75.0" text="STATUS" />
                    <TableColumn fx:id="actions" minWidth="193.6" prefWidth="75.0" text="ACTIONS" />
                </columns>
            </TableView>
   <TextField fx:id="search_even" layoutX="14.0" layoutY="48.0" prefHeight="33.0" prefWidth="736.0" styleClass="search_Select" promptText="Rechercher un Evenement..." />
   <Button fx:id="btn_search" layoutX="908.0" layoutY="48.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="60.0" styleClass="btn_filter" onAction="#getevent" text="Filtrer" />
   <Label layoutX="14.0" layoutY="111.0" prefHeight="32.0" prefWidth="193.0" text="Evenments List">
      <font>
         <Font name="Arial Bold" size="24.0" />
      </font>
   </Label>
   <Button fx:id="add_event" layoutX="813.0" layoutY="112.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="141.0" styleClass="add_event" text="Ajoute Evenement" />
        

     </AnchorPane>
</StackPane>
